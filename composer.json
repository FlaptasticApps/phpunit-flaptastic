{
    "name": "flaptasticapps/flaptastic",
    "description": "Flaptastic py.test plugin.",
    "keywords": ["PHPUnit", "assertion", "listener"],
    "homepage": "https://github.com/FlaptasticApps/phpunit-flaptastic",
    "type": "library",
    "license": "WTFPL",
    "authors": [
        {
            "name": "Jonathan Block",
            "homepage": "https://www.flaptastic.com/"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "guzzlehttp/guzzle": ">=6.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
            "FlaptasticApps\\PHPUnit\\Listener\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FlaptasticApps\\PHPUnit\\Listener\\Tests\\": "tests/"
        }
    }
}
