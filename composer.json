{
    "name": "blockjon/flaptastic",
    "description": "This PHPUnit extension uploads your test results to https://www.flaptastic.com/ to help you stop test flapping when run in any proper CICD system such as CircleCI, TravisCI, or Jenkins.",
    "keywords": ["PHPUnit", "assertion", "listener"],
    "homepage": "https://github.com/blockjon/phpunit-flaptastic",
    "type": "library",
    "license": "WTFPL",
    "authors": [
        {
            "name": "Jonathan Block",
            "homepage": "https://www.flaptastic.com/"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.4.4"
    },
    "autoload": {
        "psr-4": {
            "BlockJon\\PHPUnit\\Listener\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BlockJon\\PHPUnit\\Listener\\Tests\\": "tests/"
        }
    }
}
